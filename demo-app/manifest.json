{
	"manifest_version":1,
	"type":"WebApp",
	"meta":{
		"name":"Experiment One",
		"version":"0.0.1",
		"minimum-overwolf-version":"0.77.10",
		"author":"Tyson Taylor",
		"icon":"IconMouseOver.png",
		"icon_gray":"IconMouseNormal.png",
		"description":"My first experimental exporation of the Overwolf api"
	},
	"data": {
		"start_window":"MainWindow",
		"windows":{
			"MainWindow":{
					"file":"Files/index.html",
					"transparent":true,
					"resizable":true,
					"size":{"width":400, "height":300},
					"min_size":{"width":200, "height":200},
					"max_size":{"width":600, "height":500}
				},
			"SubWindow":{
					"file":"Files/subwindow.html",
					"transparent":true,
					"resizable":false,
					"size":{"width":400, "height":300},
					"in_game_only":true
				}
		}
	}
}